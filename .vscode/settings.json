{
    "rust-analyzer.cargo.target": "wasm32-unknown-unknown",
    "rust-analyzer.checkOnSave.command": "clippy",
    "rust-analyzer.cargo.features": "all",
    "rust-analyzer.cargo.allTargets": true,
    "rust-analyzer.rustfmt.extraArgs": [
        "+nightly"
    ],
    "rust-analyzer.diagnostics.enable": true,
    "rust-analyzer.diagnostics.enableExperimental": true,
    "rust-analyzer.completion.addCallParentheses": true,
    "rust-analyzer.completion.addCallArgumentSnippets": true,
    "rust-analyzer.inlayHints.enable": true,
    "rust-analyzer.inlayHints.chainingHints": true,
    "rust-analyzer.inlayHints.parameterHints": true,
    "rust-analyzer.inlayHints.typeHints": true,
    "rust-analyzer.lens.enable": true,
    "rust-analyzer.lens.run": true,
    "rust-analyzer.lens.debug": true,
    "rust-analyzer.lens.implementations": true,
    "rust-analyzer.lens.references": true,
    "rust-analyzer.hover.actions.enable": true,
    "rust-analyzer.hover.actions.implementations": true,
    "rust-analyzer.hover.actions.references": true,
    "rust-analyzer.hover.actions.run": true,
    "rust-analyzer.hover.actions.debug": true,
    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust-analyzer",
        "editor.formatOnSave": true,
        "editor.semanticHighlighting.enabled": true
    },
    "[toml]": {
        "editor.formatOnSave": true
    },
    "files.watcherExclude": {
        "**/target/**": true
    },
    "search.exclude": {
        "**/target": true,
        "**/Cargo.lock": true
    },
    "files.associations": {
        "*.rs": "rust"
    },
    "terminal.integrated.env.linux": {
        "RUST_BACKTRACE": "1"
    },
    "terminal.integrated.env.osx": {
        "RUST_BACKTRACE": "1"
    },
    "terminal.integrated.env.windows": {
        "RUST_BACKTRACE": "1"
    }
}